
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

    @*Burada PartialView kullanılmıştır.!!*@
    @*@Html.PartialAsync("~/views/partials/partial_header.cshtml/")*@
    <partial name="~/views/partials/partial_header.cshtml" />
<body>
    @*Burada PartialView kullanılmıştır.!!*@
    <partial name="~/views/partials/partial_navbar.cshtml" />

    <div id="layoutSidenav">

        @*Burada PartialView kullanılmıştır.!!*@
        <partial name="~/views/partials/partial_sidebar.cshtml" />

        <div id="layoutSidenav_content">
            <main>
                <div class="container-fluid px-4">
                    <h1 class="mt-4">Static Navigation</h1>
                    @RenderBody()
                </div>
            </main>
            @*Burada PartialView kullanılmıştır.!!*@
            <partial name="~/views/partials/partial_footer.cshtml" />

        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="~/tema/js/scripts.js"></script>
</body>
</html>


